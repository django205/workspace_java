package pro;

public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	for(int i=1;i<=10;i++){
		System.out.println();
		for(int j=1;j<=20;j++){
			if(j<i||j>21-i) System.out.print("*");
			else System.out.print(" ");
			
		}
	}
	
	for(int k=10;k>=1;k--){
		System.out.println();
		for(int l=10;l>=1;l--){
			if(l>k) System.out.print("$");
			else System.out.print("*");
		}
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	}}
